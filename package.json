{
  "name": "mysite",
  "version": "1.0.0",
  "description": "Jenovic's website",
  "main": "index.tsx",
  "repository": "https://github.com/Jenovic/Mysite.git",
  "author": "Jenovic Mbuyi Lumu",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm:api:dev\" \"npm:app:dev\" -n api,app",
    "api:dev": "cross-env NODE_ENV=development nodemon",
    "api:prod": "node_modules/.bin/cross-env NODE_ENV=production ts-node --files ./src/api/index.ts",
    "app:dev": "webpack-dev-server --config webpack.config.dev.js",
    "app:prod": "webpack --config webpack.config.prod.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "make:migration": "node_modules/.bin/sequelize migration:generate --name",
    "test": "cross-env NODE_ENV=test ts-mocha src/tests/**/*.test.ts"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "bulma": "^0.7.5",
    "mobx-react": "^6.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.1",
    "react-typed": "^1.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/mocha": "^5.2.6",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/sharp": "^0.22.1",
    "@types/swagger-schema-official": "^2.0.14",
    "autoprefixer": "^9.4.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.0.1",
    "tslint": "^5.13.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-loader": "^3.5.4",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
